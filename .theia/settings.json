{
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/scripts/generated/**": true,
    "**/.ui/**": true
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.autoSave": "off",
  "application.confirmExit": "ifRequired",
  "workspace.preserveWindow": true
}
