{
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/**": true,
        "**/dist/**": true,
        "**/.smartface/generated/**": true,
        "**/.smartface/ui/**": true
    },
    "editor.autoSave": "off",
    "application.confirmExit": "ifRequired",
    "workspace.preserveWindow": true,
    "explorer.autoReveal": true
}