{
    "name": "helloworld-boilerplate",
    "description": "This is Smartface's helloworld boilerplate project",
    "author": {
        "name": "Smartface",
        "email": "info@smartface.io",
        "url": "http://smartface.io"
    },
    "version": "0.0.1",
    "private": true,
    "keywords": [
        "smartface",
        "javascript",
        "typescript",
        "boilerplate",
        "mobile",
        "cross-platform",
        "native"
    ],
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios ",
        "metro": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "convert-svg": "npx @svgr/cli -- icons/test.svg > icons/test.js",
        "svgr": "svgr ./app/assets/svgs -d ./app/assets/icons --native --icon - --ignore-existing",
        "build:assets": "npx react-native-asset",
        "postinstall": "transpiler --standalone --rootPath=$(pwd)"
    },
    "dependencies": {
        "@react-navigation/bottom-tabs": "^6.5.7",
        "@react-navigation/native": "^6.1.6",
        "@react-navigation/native-stack": "^6.9.12",
        "@reduxjs/toolkit": "^1.9.5",
        "@smartface/builder": "1.0.0-beta.36",
        "@smartface/dispatcher": "1.0.0-beta.36",
        "@testing-library/jest-native": "^5.4.2",
        "@testing-library/react-native": "^12.1.2",
        "axios": "^1.4.0",
        "babel-plugin-react-native-web": "^0.18.12",
        "csv-parse": "^5.3.0",
        "i18next": "^23.2.11",
        "react": "18.2.0",
        "react-dom": "^18.2.0",
        "react-i18next": "^13.0.2",
        "react-native": "0.73.0",
        "react-native-gesture-handler": "^2.12.0",
        "react-native-keyboard-aware-scroll-view": "^0.9.5",
        "react-native-paper": "^4.12.4",
        "react-native-reanimated": "^3.3.0",
        "react-native-safe-area-context": "^4.5.3",
        "react-native-screens": "^3.20.0",
        "react-native-svg": "^13.9.0",
        "react-native-web": "^0.18.12",
        "react-redux": "^8.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@react-native-community/eslint-config": "^2.0.0",
        "@react-native/babel-preset": "^0.73.18",
        "@react-native/eslint-config": "^0.73.1",
        "@react-native/metro-config": "^0.73.2",
        "@react-native/typescript-config": "^0.73.1",
        "@rtk-incubator/rtk-query-codegen-openapi": "^0.5.1",
        "@rtk-query/codegen-openapi": "^1.0.0",
        "@svgr/cli": "^7.0.0",
        "@tsconfig/react-native": "^2.0.2",
        "@types/jest": "^26.0.23",
        "@types/react": "^18.2.6",
        "@types/react-native": "^0.69.3",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "babel-jest": "^29.6.3",
        "babel-plugin-module-resolver": "^5.0.0",
        "esbuild-runner": "^2.2.2",
        "eslint": "^8.19.0",
        "jest": "^29.6.3",
        "metro-react-native-babel-preset": "^0.70.3",
        "prettier": "2.8.8",
        "react-native-asset": "^2.1.1",
        "react-test-renderer": "18.2.0",
        "ts-node": "^10.9.1",
        "typescript": "5.0.4"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ]
    }
}