{
  "#page1": {
    "flexProps": {
      "justifyContent": "SPACE_BETWEEN",
      "alignItems": "STRETCH"
    },
    "paddingBottom": 20,
    "paddingLeft": 16,
    "paddingRight": 16,
    "&-lbl": {
      "backgroundColor": "rgba( 255, 255, 255, 0 )",
      "flexProps": {
        "marginLeft": 20,
        "marginRight": 20,
        "alignSelf": "STRETCH"
      },
      "font": {
        "size": 32,
        "bold": false,
        "italic": false,
        "family": "SFProText",
        "style": "Regular"
      },
      "multiline": true,
      "textAlignment": "MIDCENTER",
      "textColor": "${mainTextColor}"
    },
    "&-btnNext": {
      "flexProps": {
        "alignSelf": "AUTO",
        "positionType": "RELATIVE"
      },
      "width": null
    },
    "&-wrapper": {
      "flexProps": {
        "flexGrow": 1,
        "justifyContent": "CENTER"
      },
      "height": null,
      "width": null
    }
  }
}